{
  "name": "universal-consolejsjs",
  "version": "1.0.0",
  "description": "Runtime toggle to disable/enable console.* globally (browser + node). Supports allow-lists, logOnce, temporary enabling, CLI and plugin skeletons.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "universal-consolejs": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "pnpm build || npm run build",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "console",
    "production",
    "debug",
    "logging",
    "universal-consolejs"
  ],
  "author": "Officialryder",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.5.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0"
  }
}
